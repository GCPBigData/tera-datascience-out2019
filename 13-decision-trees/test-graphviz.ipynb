   "source": [
    "import graphviz\n",
    "\n",
    "d = graphviz.Digraph()\n",
    "\n",
    "with d.subgraph() as s:\n",
    "    s.attr(rank='same')\n",
    "    s.node('A')\n",
    "    s.node('X')\n",
    "\n",
    "d.node('C')\n",
    "\n",
    "with d.subgraph() as s:\n",
    "    s.attr(rank='same')\n",
    "    s.node('B')\n",
    "    s.node('D')\n",
    "    s.node('Y')\n",
    "\n",
    "d.edges(['AB', 'AC', 'CD', 'XY'])\n",
    "\n",
    "d.view()"
   ]
